{
  "name": "@tsxmod/utils",
  "version": "0.0.0",
  "description": "Utilities to help make TS Morph even easier to work with.",
  "author": "Travis Arnold",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/souporserious/tsxmod/tree/main/packages/utils"
  },
  "main": "dist/client/index.js",
  "module": "dist/client/index.mjs",
  "typings": "dist/client/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    },
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/client/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "ast",
    "ts",
    "explorer",
    "typescript",
    "refactor",
    "analyze",
    "analysis",
    "codemod",
    "utilities"
  ],
  "scripts": {
    "dev": "tsup client/index.ts server/index.ts --dts --sourcemap --format esm,cjs --watch",
    "build": "tsup client/index.ts server/index.ts --dts --format esm,cjs",
    "test": "jest"
  },
  "peerDependencies": {
    "ts-morph": ">=17.0.0"
  },
  "devDependencies": {
    "clsx": "2.0.0",
    "ts-morph": "^21.0.1"
  },
  "dependencies": {
    "tsconfig-paths": "^4.2.0"
  }
}
